{
  "author": "Pony Unicorn",
  "name": "web3-framework",
  "version": "0.0.1",
  "private": false,
  "license": "MIT",
  "description": "Web3 App framework built using Next.js, RainbowKit and more! ",
  "keywords": [
    "Web3",
    "Dapp",
    "Wallet",
    "Ethereum",
    "Framework",
    "Next.js",
    "RainbowKit"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint '**/*.{ts,tsx}'",
    "lint-fix": "eslint '**/*.{ts,tsx}' --fix",
    "format": "prettier . --list-different",
    "format-fix": "prettier . --write",
    "prepare": "husky install ; prisma generate ; wagmi generate"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src": [
      "lint",
      "prettier"
    ]
  },
  "engines": {
    "npm": "=10.2.5",
    "node": "=20.10.0"
  },
  "packageManager": "pnpm@8.12.1",
  "dependencies": {
